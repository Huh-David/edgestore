{
  "name": "@edge-store/react",
  "version": "0.0.0-alpha.7",
  "description": "Image Handling for React/Next.js",
  "homepage": "https://edge-store.com",
  "repository": "https://github.com/edgestorejs/edge-store.git",
  "author": "Ravi <me@ravi.com>",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "react",
    "nodejs",
    "nextjs",
    "image",
    "cdn",
    "edgestore",
    "edge-store"
  ],
  "exports": {
    ".": "./index.js",
    "./next": "./next/index.js",
    "./providers": "./providers/index.js"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf next libs providers index.d.ts index.d.ts.map index.js",
    "watch": "npm run clean && tsc -w",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "next",
    "libs",
    "providers",
    "index.d.ts",
    "index.js"
  ],
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.294.0",
    "@aws-sdk/s3-request-presigner": "^3.294.0",
    "@panva/hkdf": "^1.0.4",
    "cookie": "^0.5.0",
    "jose": "^4.13.1",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "next": "^13 || ^12",
    "react": "^17.0.2 || ^18",
    "react-dom": "^17.0.2 || ^18"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.1",
    "next": "^13",
    "react": "^18",
    "react-dom": "^18",
    "tsconfig": "*"
  },
  "engines": {
    "node": "^16.13.0 || ^18.12.0"
  }
}
