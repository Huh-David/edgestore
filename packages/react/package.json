{
  "name": "@edge-store/react",
  "version": "0.0.0-alpha.8",
  "description": "Image Handling for React/Next.js",
  "homepage": "https://edge-store.com",
  "repository": "https://github.com/edgestorejs/edge-store.git",
  "author": "Ravi <me@ravi.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "keywords": [
    "react",
    "nodejs",
    "nextjs",
    "image",
    "cdn",
    "edgestore",
    "edge-store"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin rollup-plugin-swc3",
    "clean": "rm -rf libs server index.d.ts index.d.ts.map index.js",
    "watch": "npm run clean && tsc -w",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./server": {
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./server/adapters/next": {
      "import": "./dist/server/adapters/next/index.mjs",
      "require": "./dist/server/adapters/next/index.js",
      "default": "./dist/server/adapters/next/index.js"
    },
    "./server/providers": {
      "import": "./dist/server/providers/index.mjs",
      "require": "./dist/server/providers/index.js",
      "default": "./dist/server/providers/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json",
    "adapters",
    "providers",
    "server",
    "!**/*.test.*"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.294.0",
    "@aws-sdk/s3-request-presigner": "^3.294.0",
    "@panva/hkdf": "^1.0.4",
    "cookie": "^0.5.0",
    "jose": "^4.13.1",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "next": "^13 || ^12",
    "react": "^17.0.2 || ^18",
    "react-dom": "^17.0.2 || ^18",
    "zod": "^3"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.1",
    "next": "^13",
    "react": "^18",
    "react-dom": "^18",
    "typescript": "^5.1.6"
  }
}
