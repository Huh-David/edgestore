  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.11.18",
    "next": "^13.0.0",
    "tsconfig": "*"
  },
  "engines": {
    "node": "^16.13.0 || ^18.12.0"
  }
