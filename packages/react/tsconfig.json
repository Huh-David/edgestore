{
  "extends": "tsconfig/base.json",
  "include": ["**/*.ts", "**/*.tsx"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "outDir": ".",
    "rootDir": "src",
    "paths": {
      "next/*": ["../next/*"],
      "providers/*": ["../providers/*"]
    }
  },
  "exclude": ["node_modules", "./*.js", "./*.d.ts", "providers", "next", "libs"]
}
